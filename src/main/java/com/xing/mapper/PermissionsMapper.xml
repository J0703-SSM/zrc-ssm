<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xing.mapper.PermissionsMapper">
    <insert id="addPermissions" parameterType="com.xing.domin.Permissions">
        INSERT INTO permissions (preId,preName,perContent) VALUE (#{preId},#{preName},#{perContent})
    </insert>
    <select id="findAllPer" resultType="com.xing.domin.Permissions">
        SELECT * FROM permissions;
    </select>
</mapper>